{
	"name": "@gam-test/fetch-wrapper",
	"version": "0.2.7",
	"description": "A simple fetch wrapper for better error handling and less response context",
	"type": "commonjs",
	"files": [
		"lib",
		"dist",
		"index.d.ts",
		"package.json"
	],
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"import": "./lib/mjs/index.js",
			"require": "./lib/cjs/index.js"
		},
		"./*": {
			"types": "./lib/*.d.ts",
			"import": "./lib/mjs/*.js",
			"require": "./lib/cjs/*.js"
		},
		"./exceptions/*": {
			"types": "./lib/exceptions/*.d.ts",
			"import": "./lib/cjs/exceptions/*.mjs",
			"require": "./lib/cjs/exceptions/*.js"
		},
		"./http/*": {
			"types": "./lib/http/*.d.ts",
			"import": "./lib/mjs/http/*.mjs",
			"require": "./lib/cjs/http/*.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "tsup",
		"build": "env NODE_ENV=production tsup"
	},
	"keywords": [
		"fetch",
		"fetch wrapper",
		"fetch error handling",
		"axios alternative"
	],
	"author": "Gustavo Atencio",
	"license": "MIT",
	"devDependencies": {
		"tsup": "8.0.2",
		"typescript": "5.3.3",
		"@types/node": "20.11.19"
	}
}