{
	"name": "@gam-test/fetch-wrapper",
	"version": "0.1.2",
	"description": "A simple fetch wrapper for better error handling and less response context",
	"type": "module",
	"files": [
		"lib",
		"dist"
	],
	"exports": {
		"types": "./lib/index.d.ts",
		"import": "./lib/index.js",
		"require": "./lib/index.cjs"
	},
	"scripts": {
		"dev": "tsup",
		"build": "env NODE_ENV=production tsup"
	},
	"keywords": [
		"fetch",
		"fetch wrapper",
		"fetch error handling",
		"axios alternative"
	],
	"author": "Gustavo Atencio",
	"license": "MIT",
	"devDependencies": {
		"tsup": "8.0.2",
		"typescript": "5.3.3",
		"@types/node": "20.11.19"
	}
}
