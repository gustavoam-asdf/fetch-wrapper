{
	"name": "@gam-test/fetch-wrapper",
	"version": "0.2.9",
	"description": "A simple fetch wrapper for better error handling and less response context",
	"type": "commonjs",
	"files": [
		"dist",
		"cjs",
		"mjs",
		"**/*.d.ts",
		"package.json"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./mjs/index.js",
			"require": "./cjs/index.js"
		},
		"./*": {
			"types": "./*.d.ts",
			"import": "./mjs/*.js",
			"require": "./cjs/*.js"
		},
		"./exceptions/*": {
			"types": "./exceptions/*.d.ts",
			"import": "./cjs/exceptions/*.mjs",
			"require": "./cjs/exceptions/*.js"
		},
		"./http/*": {
			"types": "./http/*.d.ts",
			"import": "./mjs/http/*.mjs",
			"require": "./cjs/http/*.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "env NODE_ENV=production tsup"
	},
	"keywords": [
		"fetch",
		"fetch wrapper",
		"fetch error handling",
		"axios alternative"
	],
	"author": "Gustavo Atencio",
	"license": "MIT",
	"devDependencies": {
		"tsup": "8.0.2",
		"typescript": "5.3.3",
		"@types/node": "20.11.19"
	}
}