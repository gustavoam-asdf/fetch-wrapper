{
	"name": "@gam-test/fetch-wrapper",
	"version": "0.1.4",
	"description": "A simple fetch wrapper for better error handling and less response context",
	"type": "module",
	"files": [
		"lib",
		"dist",
		"index.d.ts",
		"package.json"
	],
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		},
		"./exceptions/*": {
			"types": "./lib/exceptions/*.d.ts",
			"import": "./lib/exceptions/*.js",
			"require": "./lib/exceptions/*.cjs"
		},
		"./http/*": {
			"types": "./lib/http/*.d.ts",
			"import": "./lib/http/*.js",
			"require": "./lib/http/*.cjs"
		}
	},
	"scripts": {
		"dev": "tsup",
		"build": "env NODE_ENV=production tsup"
	},
	"keywords": [
		"fetch",
		"fetch wrapper",
		"fetch error handling",
		"axios alternative"
	],
	"author": "Gustavo Atencio",
	"license": "MIT",
	"devDependencies": {
		"tsup": "8.0.2",
		"typescript": "5.3.3",
		"@types/node": "20.11.19"
	}
}